<configuration>
	<system.serviceModel>
		<bindings>
			<netTcpBinding>
				<binding name="tcpBinding"
						 maxReceivedMessageSize="65536"
						 transferMode="Streamed" />
			</netTcpBinding>
		</bindings>
		<client>
			<endpoint address="net.tcp://localhost:9000/BatteryService"
					  binding="netTcpBinding"
					  bindingConfiguration="tcpBinding"
					  contract="Common.IBatteryService"
					  name="BatteryServiceEndpoint"/>
		</client>
	</system.serviceModel>
</configuration>