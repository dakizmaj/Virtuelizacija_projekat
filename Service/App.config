<configuration>
	<appSettings>
		<!-- pragovi: možeš prilagoditi -->
		<add key="V_threshold" value="0.02" />
		<add key="Z_threshold" value="0.01" />
		<add key="PercentThreshold" value="0.25" />
	</appSettings>
	<system.serviceModel>
		<services>
			<service name="Service.BatteryService">
				<endpoint
					address="net.tcp://localhost:9000/BatteryService"
					binding="netTcpBinding"
					contract="Common.IBatteryService" />
				<host>
					<baseAddresses>
						<add baseAddress="net.tcp://localhost:9000/BatteryService"/>
					</baseAddresses>
				</host>
			</service>
		</services>
		<bindings>
			<netTcpBinding>
				<binding name="tcpBinding" transferMode="Buffered"
						 maxReceivedMessageSize="65536" />
			</netTcpBinding>
		</bindings>
	</system.serviceModel>
	<!--
	<appSettings>
		<add key="V_treshold" value="0.0001"/>
		<add key="Z_treshold" value="3.25"/>
	</appSettings>
	-->
</configuration>